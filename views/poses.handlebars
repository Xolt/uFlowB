<html>

<head>
	<title>UFlow</title>
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/uFlow.css" rel="stylesheet">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>

<body>
	<div class= "curmenu">
		<h1 id="myInfo"><a href='/information'>
			<span class="glyphicon glyphicon-info-sign"></span></a>
		</h1>
		<h1 id="hPoses">POSES</h1>
		<h1 id="mySet"><a href='/settings'> 
			<span class="glyphicon glyphicon-cog"></span></a>
      	</h1>
	</div>

	{{#each poses}}
	<p><center>{{name}}</center></p>
	<hr><center><img width= "200" height= "200" src= "{{imgURL}}"></center></hr>
	<hr><center><iframe width="300" height="315" src="{{videoURL}}"></iframe></center></hr>
	{{/each}}
	<!--this makes the table go under under stuff for some rsn<div class= "progress"--> 
	
	<table style= "width:100%">
	<tr>
		<td><h4 id= "myProfile"><a href='/index'>PROFILE</a></h4></td>
		<td><h4 id= "myFlow"><a href='/flow'>FLOW</a></h4></td>
		<td><h4 id= "myPoses"><a href='/poses'>POSES</a></h4></td>
	</tr>
	</table>
	</div>
</body>

</html>